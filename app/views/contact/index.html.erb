<h1>Contact</h1>


<%= form_tag("/search", method: "post") do %>
  <%= label_tag(:q, "Search for Contact:") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag("Search") %>
<% end %>


<ul>
  <% @contacts.each do |contact| %>
  <li>
    <%= link_to contact.name, "/identity/#{contact.id}"%>
  </li>
  <%end%>
</ul>

<%= link_to "New Contact", "/new_contact" %>

<%= link_to "Favourite", "/my_favourite"%>
